{
  "$schema": "https://biomejs.dev/schemas/1.5.2/schema.json",
  "organizeImports": {
    "enabled": true
  },
  "formatter": {
    "enabled": true,
    "lineWidth": 120,
    "indentStyle": "space",
    "indentWidth": 2,
    "formatWithErrors": true
  },
  "javascript": {
    "globals": ["ObjC", "Application", "delay", "Path", "$"]
  },
  "linter": {
    "enabled": true,
    "rules": {
      "all": true
    }
  }
}
