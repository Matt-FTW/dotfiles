      "on-click": "kitty --class center-float-large topgrade -k --only system",
      "on-click-right": "kitty --class center-float-large topgrade -k --no-self-update"
    },
    "custom/arch": {
      "format": " ÔåÉ ",
      "tooltip": false,
      "on-click": "sleep 0.1 && rofi -show drun",
