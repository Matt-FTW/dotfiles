    "gtk-layer-shell": true,
    "margin-top": 10,
    "margin-left": 17,
    "margin-bottom": 0,
    "margin-right": 17,
    "modules-left": [
      "custom/arch",
      "hyprland/workspaces",
      "custom/weather",
      "custom/wf-recorder"
    ],
    "modules-center": ["hyprland/window"],
    "modules-right": [
      "custom/updates",
      "hyprland/language",
      "clock",
      "pulseaudio",
      "custom/notifications"
    ],
    "hyprland/window": {
      "format": "{}",
      "max-length": 80,
      "rewrite": {
        "(.*) ‚Äî LibreWolf": "Û∞áß $1",
        "(.*) - Discord": "Û∞ôØ $1",
        "(.*) - fish": "> [$1]"
      },
      "separate-outputs": false
    },
    "custom/wf-recorder": {
      "format": "{}",
      "interval": "once",
      "exec": "echo 'Ôäç'",
      "tooltip": "false",
      "exec-if": "pgrep 'wf-recorder'",
      "on-click": "exec $HOME/.config/hypr/scripts/rofi/recording",
      "signal": 8
    },
    "custom/updates": {
      "format": "{} {icon}",
      "return-type": "json",
      "format-icons": {
        "has-updates": "Û±ç∑",
        "updated": "Û∞Ç™"
